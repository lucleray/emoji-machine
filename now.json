{
  "name": "emoji-machine",
  "type": "static",
  "alias": "emoji-machine.now.sh",
  "static": {
    "directoryListing": false,
    "trailingSlash": true,
    "cleanUrls": true,
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=3600, s-maxage=31536000"
          }
        ]
      },
      {
        "source": "_next/**/*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
}
